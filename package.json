{
  "name": "linter-ada",
  "main": "./lib/init",
  "version": "0.1.0",
  "description": "Linter for Ada use GNATMake",
  "repository": "https://github.com/soiamsoNG/linter-ada",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "package-deps": [
    "linter",
    "language-ada"
  ],
  "coffeelintConfig": {
    "indentation" : {
        "level" : "error",
        "value" : 2
    },
    "line_endings" : {
        "value" : "unix",
        "level" : "error"
    }
  }
}
